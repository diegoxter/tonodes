{
    "compilerOptions": {
        "checkJs": false,
        "target": "ES2020",
        "outDir": "dist",
        "module": "commonjs",
        "declaration": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "strictNullChecks": false
    },
    "exclude": ["node_modules"]
}
